
def RangeSumB(n,arr):
    q = int(input("Enter number of times:"))
    for i in range(0,q):
        i = int(input("Enter the starting index:"))
        j = int(input("Enter the ending index:"))
        sum = 0
        for j in range(i,j+1):
            sum += arr[j]
        print("The range sum is:",sum)
    return sum

def RangeSumO(size,arr):
    prefix = [0]*(size+1)
    for i in range(1, size+1):
        prefix[i] = prefix[i-1] + arr[i-1]
    q = int(input("Enter number of times:"))
    for _ in range(q):
        i = int(input("Enter the starting index:"))
        j = int(input("Enter the ending index:"))
        range_sum = prefix[j+1] - prefix[i]
        print("The range sum is:",range_sum)
    

size = int(input("Enter the size of array:"))
arr = []
for i in range(0,size):
    arr.append(int(input("Enter the element:")))
print(RangeSumO(size, arr))